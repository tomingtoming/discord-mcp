{
  "name": "discord-mcp",
  "version": "0.1.0",
  "description": "Model Context Protocol server for Discord integration",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "discord-mcp": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "node --loader ts-node/esm src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "modelcontextprotocol",
    "discord",
    "discord-bot",
    "ai",
    "assistant",
    "llm"
  ],
  "author": "",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomingtoming/discord-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/tomingtoming/discord-mcp/issues"
  },
  "homepage": "https://github.com/tomingtoming/discord-mcp#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.6.1",
    "discord.js": "^14.19.3"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
